{
  "name": "circlebe",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "packageManager": "pnpm@9.0.5",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "heroku-postbuild": "ppm run build",
    "test": "pnpm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register test/**/*.ts",
    "start": "pnpm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "db:migrate:dev": "prisma migrate dev --name init",
    "db:migrate:prod": "dotenv -e .env -- prisma migrate deploy",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js --port 5000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/sensible": "^5.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@fastify/websocket": "^10.0.1",
    "@joggr/fastify-prisma": "^1.2.3",
    "@sinclair/typebox": "^0.32.22",
    "@types/node": "^20.4.4",
    "@types/ws": "^8.5.10",
    "c8": "^9.0.0",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.1",
    "env-schema": "^5.2.1",
    "fastify": "^4.26.1",
    "fastify-bcrypt": "^1.0.1",
    "fastify-cli": "^6.1.1",
    "fastify-plugin": "^4.0.0",
    "fastify-tsconfig": "^2.0.0",
    "prisma": "^5.12.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "zod": "^3.23.6"
  }
}
